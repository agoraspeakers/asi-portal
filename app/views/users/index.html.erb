<p id="notice"><%= notice %></p>

<h1>Users</h1>

<table class="table table-striped table-bordered table-hover">
  <thead>
    <tr>
      <th>Name</th>
    </tr>
  </thead>

  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= link_to user.email, user %></td>
      </tr>
    <% end %>
  </tbody>
</table>
